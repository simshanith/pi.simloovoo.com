{
  "buildCommand": "yarn build",
  "src": {
    "beforeStartCommand": "yarn lint:summary",
    "afterReleaseCommand": "yarn deploy",
    "commitMessage": "Release v%s",
    "tagName": "v%s",
    "tagAnnotation": "Release v%s"
  },
  "github": {
    "release": true,
    "releaseName": "v%s",
    "assets": ["build.zip", "build.tar.gz"]
  },
  "npm": {
    "publish": false
  },
  "prompt": {
    "src": {
      "publish": false
    }
  }
}
