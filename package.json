{
  "name": "pi.simloovoo.com",
  "version": "1.9.0",
  "description": "static website deployed on a Raspberry Pi",
  "repository": {
    "url": "https://github.com/simshanith/pi.simloovoo.com.git",
    "type": "git"
  },
  "author": {
    "email": "simrpgman@gmail.com",
    "name": "Shane Daniel"
  },
  "license": "MIT",
  "private": true,
  "scripts": {
    "build:clean": "rimraf build build.zip build.tar.gz",
    "build:scaffold": "mkdirp build && cp CNAME build/",
    "build:webpack": "COMMIT_MESSAGE=\"$(./bin/commit-message)\" webpack --env.production --progress --profile --colors",
    "build": "npm run build:clean && npm run build:scaffold && npm run build:webpack",
    "dev": "webpack-dev-server --progress --profile --colors",
    "deploy": "npm run deploy:gh-pages",
    "deploy:pi": "./bin/deploy-nginx.sh",
    "deploy:gh-pages": "gh-pages --dist build --message \"$(./bin/commit-message)\"",
    "lint": "eslint . --ext js,jsx,json",
    "lint:summary": "npm run lint -- -f tap 2>/dev/null | faucet",
    "start": "webpack-dashboard -- npm run dev",
    "release-it": "node -r dotenv-safe/config node_modules/.bin/release-it"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
