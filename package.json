{
  "name": "pi.simloovoo.com",
  "version": "1.0.0",
  "description": "static website deployed on a Raspberry Pi",
  "repository": {
    "url": "git@github.com:simshanith/pi.simloovoo.com.git",
    "type": "git"
  },
  "author": "Shane Daniel <simrpgman@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build:scaffold": "mkdirp build",
    "build:stats": "webpack --env.production --progress --profile --colors --json > build/webpack-stats.json",
    "build": "npm run build:scaffold && npm run build:stats",
    "clean": "rimraf build",
    "dev": "npm start",
    "lint": "eslint .",
    "start": "webpack-dashboard -- npm run webpack-dev-server",
    "webpack-dev-server": "webpack-dev-server --progress --profile --colors"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-react-transform": "^3.0.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^4.8.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-react": "^7.4.0",
    "glob": "^7.1.2",
    "html-webpack-plugin": "^2.30.1",
    "mkdirp": "^0.5.1",
    "pug": "^2.0.0-rc.4",
    "pug-loader": "^2.3.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "rimraf": "^2.6.2",
    "webpack": "^3.6.0",
    "webpack-dashboard": "^1.0.0-5",
    "webpack-dev-server": "^2.9.1",
    "yaml-loader": "^0.5.0"
  }
}
