{
  "name": "pi.simloovoo.com",
  "version": "1.0.13",
  "description": "static website deployed on a Raspberry Pi",
  "repository": {
    "url": "https://github.com/simshanith/pi.simloovoo.com.git",
    "type": "git"
  },
  "author": {
    "email": "simrpgman@gmail.com",
    "name": "Shane Daniel"
  },
  "license": "MIT",
  "private": true,
  "scripts": {
    "build:scaffold": "mkdirp build",
    "build:webpack": "webpack --env.production --progress --profile --colors --json > build/webpack-stats.json",
    "build": "npm run clean && npm run build:scaffold && npm run build:webpack",
    "clean": "rimraf build build.zip build.tar.gz",
    "dev": "npm start",
    "deploy": "./bin/deploy-nginx.sh",
    "lint": "eslint . --ext js,jsx,json",
    "lint:summary": "npm run lint -- -f tap 2>/dev/null | faucet",
    "start": "webpack-dashboard -- npm run webpack-dev-server",
    "webpack-dev-server": "webpack-dev-server --progress --profile --colors",
    "release-it": "node -r dotenv/config node_modules/.bin/release-it",
    "release": "npm run release-it -- -n"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.5",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-react-transform": "^3.0.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-0": "^6.24.1",
    "child-compiler-loader-list-webpack-plugin": "^0.2.0",
    "css-loader": "^0.28.7",
    "dotenv": "^4.0.0",
    "eslint": "^4.8.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-react": "^7.4.0",
    "extract-text-webpack-plugin": "^3.0.1",
    "faucet": "^0.0.1",
    "file-loader": "^1.1.5",
    "glob": "^7.1.2",
    "history": "^4.7.2",
    "html-webpack-plugin": "^2.30.1",
    "isomorphic-style-loader": "^4.0.0",
    "marked": "^0.3.6",
    "mkdirp": "^0.5.1",
    "normalize.css": "^7.0.0",
    "postcss-flexbugs-fixes": "^3.2.0",
    "postcss-loader": "^2.0.8",
    "pug": "^2.0.0-rc.4",
    "pug-loader": "^2.3.0",
    "raw-loader": "^0.5.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "next",
    "redux": "^3.7.2",
    "release-it": "git+https://github.com/simshanith/release-it.git",
    "rimraf": "^2.6.2",
    "style-loader": "^0.19.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "webpack": "^3.6.0",
    "webpack-archive-plugin": "^3.0.0",
    "webpack-dashboard": "^1.0.0-5",
    "webpack-dev-server": "^2.9.1",
    "yaml-loader": "^0.5.0"
  }
}
